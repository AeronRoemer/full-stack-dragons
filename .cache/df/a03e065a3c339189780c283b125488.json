ed":{"line":94,"column":42},"source":"actions/account.js","original":{"line":60,"column":12}},{"generated":{"line":94,"column":53},"source":"actions/account.js","original":{"line":57,"column":44}},{"name":"ACCOUNT","generated":{"line":94,"column":55},"source":"actions/account.js","original":{"line":61,"column":4}},{"name":"FETCH_LOGOUT_SUCCESS","generated":{"line":94,"column":70},"source":"actions/account.js","original":{"line":61,"column":12}},{"generated":{"line":94,"column":90},"source":"actions/account.js","original":{"line":57,"column":44}},{"generated":{"line":94,"column":91},"source":"actions/account.js","original":{"line":57,"column":22}},{"generated":{"line":95,"column":0},"source":"actions/account.js","original":{"line":57,"column":22}},{"generated":{"line":95,"column":1},"source":"actions/account.js","original":{"line":57,"column":7}}],"sources":{"actions/account.js":"import { ACCOUNT } from './types';\n\n//double arrow funciton allows binding 'dispatch' to action creator \n//in mapDispatchToProps\nexport const fetchFromAccount = (\n    endpoint,  \n    methodOptions,\n    FETCH_TYPE, \n    ERROR_TYPE,\n    SUCCESS_TYPE\n    ) => dispatch => {\n    dispatch({ type: FETCH_TYPE });\n    console.log(FETCH_TYPE, SUCCESS_TYPE)\n    //object with extra details for POST. default is GET\n    return fetch(`http://localhost:3000/account/${endpoint}`, methodOptions).then(response => response.json())\n        .then(json => { \n            console.log(json)\n            if (json.type === 'error'){\n                dispatch({ type: ERROR_TYPE, message: json.message})\n            } else {\n                dispatch({ type: SUCCESS_TYPE, ...json })\n            }\n         })\n        .catch(error => dispatch({\n            type: ERROR_TYPE, message: error.message\n        }));\n}\n\n\nexport const signup = ({ username, password }) =>  fetchFromAccount('signup', {\n    method: 'POST',\n    body: JSON.stringify({ username, password }),\n    headers: { 'Content-Type': 'application/json' },\n    credentials: 'include'\n    },   \n    ACCOUNT.FETCH,\n    ACCOUNT.FETCH_ERROR,\n    ACCOUNT.FETCH_SUCCESS)\n\nexport const login = ({ username, password }) =>  fetchFromAccount('login', {\n        method: 'POST',\n        body: JSON.stringify({ username, password }),\n        headers: { 'Content-Type': 'application/json' },\n        credentials: 'include'\n        },\n        ACCOUNT.FETCH,\n        ACCOUNT.FETCH_ERROR,\n        ACCOUNT.FETCH_SUCCESS)\n\nexport const fetchAuthenticated = () =>  fetchFromAccount('authenticated', {\n        credentials: 'include'\n        }, \n        ACCOUNT.FETCH,\n        ACCOUNT.FETCH_ERROR,\n        ACCOUNT.FETCH_AUTHENTICATED_SUCCESS)\n\nexport const logout = () => fetchFromAccount('logout', \n    {credentials: 'include'}, \n    ACCOUNT.FETCH,\n    ACCOUNT.FETCH_ERROR,\n    ACCOUNT.FETCH_LOGOUT_SUCCESS)"},"lineCount":null}},"error":null,"hash":"64a723b76be05595c73c500ff3e16627","cacheData":{"env":{}}}
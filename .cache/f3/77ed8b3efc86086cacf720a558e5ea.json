et Error(){ //javascript computed property\n        if (this.props.account.status === fetchStates.error){\n            return <div>There was an error: {this.props.account.message}</div>\n        }\n    }\n    render(){\n        return(\n            <div>\n                <h2>Dragon Stack</h2>\n                <FormGroup>\n                    <FormControl \n                        type=\"text\"\n                        value={this.state.username}\n                        placeholder=\"username\"\n                        onChange={this.updateUsername} />\n                </FormGroup>\n                <FormGroup>\n                    <FormControl \n                        type=\"password\"\n                        value={this.state.password}\n                        placeholder=\"password\"\n                        onChange={this.updatePassword} />\n                </FormGroup>\n                <div>\n                    <Button onClick={this.login}>Log In</Button>\n                    <span>  or  </span>\n                    <Button onClick={this.signup}>Sign Up</Button>\n                </div>\n                {this.Error}\n            </div>\n        )\n    }\n}\n// first parameter is for mapStateToProps- but the state here is local\nexport default connect(({ account }) =>({ account }), { signup, login })(AuthForm);"},"lineCount":null}},"error":null,"hash":"4e0ada5954a0948c8611da5df6794daf","cacheData":{"env":{}}}